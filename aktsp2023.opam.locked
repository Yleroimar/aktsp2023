  "base-unix" {= "base"}
  "chrome-trace" {= "3.6.2"}
  "cppo" {= "1.6.9"}
  "csexp" {= "1.5.1"}
  "dune" {= "3.6.2"}
  "dune-build-info" {= "3.6.2"}
  "dune-configurator" {= "3.6.2"}
