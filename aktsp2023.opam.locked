  "ocamlc-loc" {= "3.6.2"}
  "ocamlfind" {= "1.9.5"}
  "ocamlformat-rpc-lib" {= "0.24.1"}
  "ocamlgraph" {= "2.0.0"}
  "ocplib-endian" {= "1.2"}
  "octavius" {= "1.2.2"}
  "omd" {= "1.3.2"}
  "ordering" {= "3.6.2"}
  "ounit2" {= "2.2.6"}
  "pp" {= "1.1.2"}
  "ppx_derivers" {= "1.2.1"}
  "ppx_deriving" {= "5.2.1"}
  "ppx_deriving_hash" {= "0.1.1"}
  "ppx_yojson_conv_lib" {= "v0.15.0"}
